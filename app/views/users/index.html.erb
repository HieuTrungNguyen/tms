<div class="row">
  <h2 class="users-title"><%= t ".index_title" %></h2>
  <div class="add-member">
    <%= link_to t(".btn_addUser"), new_user_path, class: "btn-add-member btn btn-info" %>
  </div>
  <div class="table-responsive">
    <table class="table table-striped table-hover">
      <thead>
        <tr>
          <th class="th-user"></th>
          <th class="th-user"><%= t "form.full_name" %></th>
          <th class="th-user"><%= t "form.email" %></th>
          <th class="th-user"><%= t "form.phone_number" %></th>
          <th class="th-user"><%= t "form.address" %></th>
          <th class="th-user"><%= t "form.role" %></th>
          <th class="th-user" colspan="2"><%= t "form.action" %></th>
        </tr>
      </thead>
      <tbody>
        <%= render @users %>
      </tbody>
    </table>
    <div class="paginate">
      <%= paginate @users, theme: "twitter-bootstrap-3" %>
    </div>
  </div>
</div>
