<div class="modal-dialog">
  <div class="modal-content">
    <div class="modal-header">
      <button type="button" class="close" data-dismiss="modal">&times;</button>
      <h4 class="modal-title"><%= t ".selected-users-title" %></h4>
    </div>
    <div class="modal-body">
      <%= render partial: "common/users_will_be_deleted_in_course",
        collection: select_users, as: :user %>
    </div>
    <div class="clearfix"></div>
    <div class="modal-footer">
      <button type="button" class="btn btn-default" data-dismiss="modal">
        <%= t "form.close" %>
      </button>
      <button type="button" class="btn btn-danger btn__delete-member js-btn__delete-member">
        <%= t "btn.delete" %>
      </button>
    </div>
  </div>
</div>
