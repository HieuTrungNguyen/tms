<div class="row">
  <h1 class="text-center"><%= t ".courses_title" %></h1>
  <div class="add-member">
    <%= link_to t(".btn_addCourse"), new_course_path, class: "btn-add-member btn btn-info" %>
  </div>
  <div class="list-group">
    <%= render @courses %>
  </div>
  <div class="paginate">
    <%= paginate @courses, theme: "twitter-bootstrap-3" %>
  </div>
</div>
